{
  "multipart": [
    { "when": { "up":    "true" }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_post"                           } },
    { "when": { "north": "low"  }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_side",           "uvlock": true } },
    { "when": { "east":  "low"  }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_side", "y":  90, "uvlock": true } },
    { "when": { "south": "low"  }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_side", "y": 180, "uvlock": true } },
    { "when": { "west":  "low"  }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_side", "y": 270, "uvlock": true } },
    { "when": { "north": "tall" }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_tall",           "uvlock": true } },
    { "when": { "east":  "tall" }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_tall", "y":  90, "uvlock": true } },
    { "when": { "south": "tall" }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_tall", "y": 180, "uvlock": true } },
    { "when": { "west":  "tall" }, "apply": { "model": "stairway:block/walls_andesite_bricks_crack_tall", "y": 270, "uvlock": true } }
  ]
}