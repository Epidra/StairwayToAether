{
    "multipart": [
        {   "when": { "up": true },
            "apply": { "model": "stairway:block/walls_concrete_lime_post" }
        },
        {   "when": { "north": true },
            "apply": { "model": "stairway:block/walls_concrete_lime_side", "uvlock": true }
        },
        {   "when": { "east": true },
            "apply": { "model": "stairway:block/walls_concrete_lime_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "stairway:block/walls_concrete_lime_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "stairway:block/walls_concrete_lime_side", "y": 270, "uvlock": true }
        }
    ]
}
